#include <iostream>
#include <iomanip>
using namespace std;
int main()
{
    //question 1
    cout<<"hello world";
    
    // question 2
    int x;
    float y;
    char z;
    cout<<"enter integer,decimal and character respectively";
    cin>>x>>y>>z;
    cout<<x<<endl<<y<<endl<<z;
    
    // question 3 
    int x;
    int y;
    cout<<"enter first number";
    cin>>x;
    cout<<"enter second number";
    cin>>y;
    int sum=x+y;
    cout<<"the sum is"<<sum<<endl;
    int subtraction= x-y;
    cout<<"the subtraction is "<<subtraction<<endl;
    int multiplication=x*y;
    cout<<"the multiplication is "<<multiplication<<endl;
    int division=x/y;
    cout<<"the division is"<<division<<endl;
    
    // question 4
    float celsius;
    cout<<"enter temperature";
    cin>>celsius;
    float F=((9*celsius)/5)+32;"temperature in farahneit is"<<F;
    
    // question 5
    int x;
    cout<<"print \n"<<"hello\t"<<"world\b"<<"goodbye"<<endl<<"enter x"<<setw(5);
    cin>>x;
    cout<<"x is"<<x;
    
    // question 6
    int x;
    int y;
    cout<<"enter value of x";
    cin>>x;
    cout<<"enter value of y";
    cin>>y;
    x+=;
    y-=;
    
    cout<<"value of x after incremation"<<x<<endl<<"value of y after decremation"<<y<<endl;
    
    // question 7
    int x;
    int y;
    cout<<"enter furst number";
    cin>>x;
    cout<<"enter 2nd number";
    cin>>y;
    x=x^y;
    y=x^y;
    x=x^y;
    cout<<x<<y;
    
    // question 8
    int x;
    cout<<"enter number of days";
    cin>>x;
    if(x>1 && x<6){
        cout<<"fine is 50 paise";
    }
    else if (x<11){
        cout<<"fine is 1 rupee";
    }
    else if(x<31){
        cout<<"fine is 5 rupees";
    }
    else{
        cout<<"membership is cancelled";
    }
    
    // question 9
    int n;
    float a,b;
    cout<<"enter two numbers";
    cin>>a>>b;
    switch(n){
        case 1:
        cout<<"addition"<<a+b;
        break;
        case 2:
        cout<<"subtraction"<<a-b;
        break;
        case 3:
        cout<<"multiplication"<<a*b;
        break;
        case 4:
        if(b!=0){
        cout<<"division"<<a/b;
        }
        else{
            cout<<"error";
        }
        break;
        default:
        cout<<"invalid input";
    }
    
    // question 10
    int n;
    cout<<"enter a number";
    cin>>n;
    n=(n%2==0)?"even":"odd";
    cout<<"the number is"<<n;

    return 0;
}